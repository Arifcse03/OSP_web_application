  <SQLQuery>
    <![CDATA[SELECT StnLineEO.HEADER_ID, 
       StnLineEO.STN_LINE_ID, 
       StnLineEO.STN, 
       StnLineEO.COLOR, 
       StnLineEO.BPO, 
       StnLineEO.PART, 
       StnLineEO.STN_QTY, 
       StnLineEO.PROD_LINE, 
       StnLineEO.CHANG_INV_ORG, 
       StnLineEO.TIME_CAL, 
       StnLineEO.CREATION_DATE, 
       StnLineEO.CREATED_BY,
       (select fu.user_name from fnd_user fu where fu.user_id = StnLineEO.CREATED_BY) Creator_Name, 
       StnLineEO.LAST_UPDATED_BY,
       (select fu.user_name from fnd_user fu where fu.user_id = StnLineEO.LAST_UPDATED_BY) Last_Updater_Name,  
       StnLineEO.ORG_ID, 
       StnLineEO.LAST_UPDATED_DATE, 
       StnLineEO.USERID, 
       StnLineEO.RESPID, 
       StnLineEO.WASH,
       StnLineEO.RESPAPPLID,
       (select j.wip_entity_name            from WIP_DISCRETE_JOBS_V j        
    where j.attribute4 in('OSP','WASHING')  
    AND j.ATTRIBUTE10= to_char(pL.System_id)
AND j.Status_Type NOT IN (7, 12)
     -- and j.attribute2 =   StnLineEO.STN      
       and j.attribute3 = StnLineEO.header_id
      -- and j.attribute8=StnLineEO.color
       and rownum = 1
       ) job_name,
       StnLineEO.SYSTEM_ID, 
       StnLineEO.POC_ID,
StnLineEO.BPO_ID,
bpo.BPO BPO_NEW,
pl.COLOR COLOR_NEW,
pl.Wash_Name New_wash,
bpo.Qty Bpo_Quantity
FROM MNJ_MFG_OSP_STN_LINE StnLineEO,xx_om_poc_h_t ph,xx_om_poc_l_t pl,xx_om_bpo_info_t bpo
where bpo.bpo_id=StnLineEO.BPO_ID
and bpo.fob_id=pl.fob_id
and pl.poc_id=ph.poc_id
ORDER BY StnLineEO.STN,StnLineEO.BPO,StnLineEO.PROD_LINE]]>
  </SQLQuery>
