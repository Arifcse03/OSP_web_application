  <SQLQuery>
    <![CDATA[SELECT SYSTEM_ID,
POC_ID,
BPO_NO,
SUM (ORDERED_QTY) ORDERED_QTY,
--STN,
COLOR,
WASH,
NULL PART,
BUYER_ID,
STYLE_NAME,
SEASON,
ORG_ID,BPO
FROM (
SELECT DISTINCT poc_l.SYSTEM_ID,
poc_h.POC_ID,
bpo.BPO_ID BPO_NO,
bpo.QTY ORDERED_QTY,
poc_l.WASH_NAME WASH,
poc_l.COLOR COLOR,
NULL PART,
poc_h.BUYER_ID,
poc_h.STYLE STYLE_NAME,
poc_h.SEASON,
poc_h.LC_UNIT ORG_ID,
bpo.BPO_ID,
bpo.BPO
FROM xx_om_bpo_info_t bpo, xx_om_poc_h_t poc_h,xx_om_poc_l_t poc_l 
WHERE bpo.CLASSIFICATION= 'BPO'
and poc_h.poc_id=poc_l.poc_id and
bpo.fob_id=poc_l.fob_id and bpo.status=1 ) ABC
WHERE NOT EXISTS
(SELECT 1
FROM MNJ_MFG_OSP_STN_LINE MMOSL, MNJ_MFG_OSP_HEADER H
WHERE 1 = 1
AND MMOSL.POC_ID = ABC.POC_ID
AND MMOSL.SYSTEM_ID = ABC.SYSTEM_ID)
GROUP BY SYSTEM_ID,
POC_ID,
BPO_NO,
COLOR,
WASH,
PART,
BUYER_ID,
STYLE_NAME,
SEASON,
ORG_ID,
BPO
ORDER BY BPO_NO]]>
  </SQLQuery>
